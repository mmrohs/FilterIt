{
	"manifest_version": 2,
	"name": "__MSG_extensionName__",
	"version": "1.0.4",
	"default_locale": "en",
	"description": "__MSG_extensionDescription__",
	
	"icons":
	{
		"48": "icons/FilterIt_48.png"
	},
	
	"content_scripts":
	[{
		"matches": ["<all_urls>"],
		"js": ["FilterIt.js"],
		"run_at": "document_end"
	}],
	
	"background": 
	{
		"scripts": ["background.js"]
	},

	"permissions": ["storage","contextMenus","menus","activeTab"]
}